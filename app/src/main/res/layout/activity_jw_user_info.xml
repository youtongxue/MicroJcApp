<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <LinearLayout        android:id="@+id/linearLayout"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:orientation="vertical">        <com.service.microjc.Activity.App.uicustomviews.TitleLayout            android:id="@+id/examTitle"            android:layout_width="match_parent"            android:layout_height="wrap_content"            app:navBackground="@color/white"            app:titleName="教务网" />        <com.scwang.smart.refresh.layout.SmartRefreshLayout            android:id="@+id/jwUserInfo_refreshLayout"            android:layout_width="match_parent"            android:layout_height="match_parent"            app:srlEnableOverScrollDrag="true"            app:srlEnablePureScrollMode="true">            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="match_parent">                <androidx.cardview.widget.CardView                    android:id="@+id/jw_card"                    android:layout_width="match_parent"                    android:layout_height="300dp"                    android:layout_alignParentStart="true"                    android:layout_alignParentTop="true"                    android:layout_alignParentEnd="true"                    android:layout_marginStart="12dp"                    android:layout_marginTop="113dp"                    android:layout_marginEnd="12dp"                    app:cardCornerRadius="12dp"                    app:cardElevation="0dp">                    <ListView                        android:id="@+id/jw_list"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:divider="@null"                        android:scrollbars="none" />                </androidx.cardview.widget.CardView>                <TextView                    android:id="@+id/textView19"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentStart="true"                    android:layout_alignParentTop="true"                    android:layout_marginStart="16dp"                    android:layout_marginTop="52dp"                    android:text="你好：" />                <TextView                    android:id="@+id/textView9"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignParentTop="true"                    android:layout_marginStart="4dp"                    android:layout_marginTop="53dp"                    android:layout_toEndOf="@+id/studentname"                    android:text="同学" />                <TextView                    android:id="@+id/studentname"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_alignTop="@+id/textView19"                    android:layout_marginStart="4dp"                    android:layout_marginTop="1dp"                    android:layout_toEndOf="@+id/textView19" />            </RelativeLayout>        </com.scwang.smart.refresh.layout.SmartRefreshLayout>    </LinearLayout></androidx.constraintlayout.widget.ConstraintLayout>